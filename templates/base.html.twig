                <div class="navbar-header">
                    <a class="navbar-brand" href="">Crowdin</a>
                </div>
                <ul class="nav nav-pills">
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('homepage') }}">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('app_login') }}">login</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('user_register') }}">sign in</a>
                    </li>
                </ul>
            </div>
        </nav>
